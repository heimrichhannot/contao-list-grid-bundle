{
    "name": "heimrichhannot/contao-list-grid-bundle",
    "type": "contao-bundle",
    "description": "Contao bundle to create highly customizable list templates that may use specific column sets, templates and image sizes for specific positions in a list.",
    "keywords": [
        "contao",
        "listgrid",
        "list",
        "grid",
        "newslist"
    ],
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/heimrichhannot/contao-list-grid-bundle",
    "authors": [
        {
            "name": "Heimrich & Hannot",
            "email": "digitales@heimrich-hannot.de",
            "homepage": "https://www.heimrich-hannot.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimrichhannot/contao-list-grid-bundle/issues"
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "contao/core-bundle": "^4.4",
        "heimrichhannot/contao-list-bundle": "^1.25"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\ContaoListGridBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\ContaoListGridBundle\\ContaoManager\\Plugin"
    }
}
